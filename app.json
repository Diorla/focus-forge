{
  "expo": {
    "name": "Focus Forge",
    "description": "A simple app to help you focus on your work and prevent procrastination.",
    "slug": "focus-forge",
    "version": "0.1.0",
    "orientation": "portrait",
    "privacy": "public",
    "icon": "./assets/images/icon.png",
    "scheme": "focus-forge",
    "userInterfaceStyle": "automatic",
    "platforms": ["android", "ios", "web"],
    "githubUrl": "https://github.com/Diorla/focus-forge",
    "primaryColor": "#6a13e3",
    "notification": {
      "icon": "./assets/images/favicon.png",
      "color": "#6a13e3",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Completed tasks"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.diorla.focusforge",
      "buildNumber": "16",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.diorla.focusforge",
      "versionCode": 18
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/favicon.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e0e861f3-9a98-45cb-84de-6076162cbe29"
      }
    }
  }
}
