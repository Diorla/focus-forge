{
  "expo": {
    "name": "Focus Forge",
    "slug": "focus-forge",
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.diorla.focusforge",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.diorla.focusforge"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e0e861f3-9a98-45cb-84de-6076162cbe29"
      }
    },
    "owner": "diorla",
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": ["./assets/tone.wav"]
        }
      ]
    ]
  }
}
